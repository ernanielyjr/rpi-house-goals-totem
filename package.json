    "@angular/language-service": "^6.1.0",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "ts-node": "~5.0.1",
    "tslint": "^5.11.0",
