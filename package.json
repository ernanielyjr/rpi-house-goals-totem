{
  "name": "budgets-totem",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "build": "ng build --prod",
    "serve-dev": "concurrently --kill-others \"npm run frontend\" \"npm run backend\"",
    "frontend": "ng build --watch",
    "backend": "nodemon ./server.js --watch ./dist --watch server.js --watch package.json"
  },
