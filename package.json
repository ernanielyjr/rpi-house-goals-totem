{
  "name": "goals-totem",
  "version": "0.0.1",
  "scripts": {
    "start": "ng build --prod && node server.js",
    "serve-dev": "concurrently --kill-others \"npm run frontend\" \"npm run backend\"",
    "frontend": "ng build --watch",
    "backend": "nodemon ./server.js --watch ./dist --watch server.js --watch package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "apicache": "^1.2.3",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.3",
